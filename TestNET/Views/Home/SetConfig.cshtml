@using TestNET.ViewModels
@model SaveModel

<div class="text-center">
    <h1 class="display-4">Results</h1>

    <h2>Source - @Model.Source</h2>

    @{ // here links output
        foreach (var link in Model.ScannedLinks)
        {
            <div>
                <a> @link </a>
            </div>
        }
    }

    <form method="post">
        <a class="btn btn-success" asp-controller="Home" asp-action="Save" asp-route-scanned=@Model.ContentString>Save</a>
    </form>
</div>