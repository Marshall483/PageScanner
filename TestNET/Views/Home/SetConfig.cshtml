@using TestNET.ViewModels
@model SaveModel

    <div class="text-center">
        <h1 class="display-4">Results</h1>

        <table class="table table-dark">
            <thead><tr><th>URL</th></tr></thead>
            <tbody>
                @{foreach(var link in Model.linksHtml)
                  {
                      <tr><th scope="row"></th>@link</tr>
                  }
                }
            </tbody>

        <form asp-controller="Home" asp-action="Save" method="post">
            <input class="btn btn-success" type="submit" value="Save" asp-controller="Home" asp-action="Save" asp-route-model=@Model>
        </form>
            
        </table>
